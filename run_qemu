
if [ -e bin/compiled ]
then
    if [ -e bin/linked ]
    then
        rm -f memdump
        qemu-system-x86_64 -smp cpus=2 -cpu max -drive file=bin/os.img,format=raw -m 6G -debugcon file:/dev/stdout -monitor stdio -no-reboot -no-shutdown
    else
        echo "linker errors!"
    fi
else
    echo "compiler errors!"
fi
